{
  "name": "oak-language",
  "displayName": "Oak Programming Language Syntax Highlighter",
  "description": "VSCode language support for Oak",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorbDehmel/oak-syntax-highlighting.git"
  },
  "publisher": "jdehmel",
  "version": "0.0.2",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "oak",
        "aliases": [
          "oak",
          "oak"
        ],
        "extensions": [
          ".oak"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "logo.png",
          "light": "logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "oak",
        "scopeName": "source.oak",
        "path": "./syntaxes/oak.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}
